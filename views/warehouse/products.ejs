                    <th scope="col-2">Stock</th>
                    <th scope="col-2">Opciones</th>
                  </tr>
                </thead>
                <tbody>
                    <% for (var product of products) { %>
                  <tr>
                    <th scope="row"><%= product.id %></th>
                    <td><%= product.name %></td>
                    <td><%= product.quantity %></td>
                    <td>Actualizar</td>
                  </tr>
                  <% } %>
                  
                </tbody>
              </table>

      

        

